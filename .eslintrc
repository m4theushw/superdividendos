{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "react-app",
    "plugin:react/recommended",
    "prettier/react"
  ],
  "plugins": ["react", "react-hooks", "prettier"],
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}
